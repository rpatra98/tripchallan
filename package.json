{
  "name": "cbums",
  "version": "0.1.0",
  "private": true,
  "description": "Updated for Vercel deployment with React 19.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && prisma migrate deploy && node prisma/seed.js && next build",
    "start": "next start",
    "lint": "next lint",
    "migrate": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "db:seed": "npx ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} prisma/seed.ts",
    "db:seed:js": "node prisma/seed.js",
    "vercel-build": "echo 'Running Vercel specific build' && prisma generate && (prisma migrate deploy || echo 'Migration failed but continuing') && (node prisma/seed.js || echo 'Seed failed but continuing') && next build"
  },
  "dependencies": {
