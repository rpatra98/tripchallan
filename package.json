    "@prisma/extension-accelerate": "^0.6.3",
    "@tanstack/react-query": "^5.17.19",
    "@types/pdfkit": "^0.13.9",
    "@types/uuid": "^10.0.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.6.1",
    "clsx": "^2.1.1",
